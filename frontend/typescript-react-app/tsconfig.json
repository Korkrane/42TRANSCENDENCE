{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    //TODO: attention a changer
    "allowJs": true,/* a changer */
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    // "noEmit": true,
    "strict": false,/* A changer ? */
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "strictNullChecks": false,//Ajout pour que canvas fonctionne
  },
  "include": [
    "src", "../Pages"
  ]
}
